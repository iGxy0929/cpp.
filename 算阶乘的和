int main()
{
	int n = 0;
	int i = 0;
	int  ret = 1;
	int sum = 0;
	//scanf_s("%d",&n);//注意scaf里面不要随便加空格
	for (n=1;n<=3;n++)
	{
		for (i=1;i<=n;i++)
		{
			ret = ret * i;//表示i×i
		}
	
		sum+=ret;//求ret的和
	}
	/*printf("ret = %d\n ", ret);*/
	printf("sum=%d\n", sum);
	return 0;
}
//更简单的如下
int main()
{
	int n = 0;
	int i = 0;
	int  ret = 1;
	int sum = 0;
	for (n = 1; n <= 3; n++)
	{

		ret = ret * n;
		sum += ret;
	}

	printf("%d\n",sum);
		return 0;
}
