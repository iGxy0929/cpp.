#include<stdio.h>
struct student{
	char name[100];
	char tel[100];
	char sex[100];
};
int main()
{
	int n=0;
	printf("请输入学生的个数：");
	scanf("%d",&n);
	student s[n];
	for(int i=0;i<n;i++)
	{
		printf("姓名:\n");
		scanf("%s",s[i].name);
		printf("电话：\n");
		scanf("%s",s[i].tel);
		printf("性别:");
		scanf("%s",s[i].sex);
		
	}
	return 0;
} 
