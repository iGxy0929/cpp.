#include<stdio.h> 
int main()
{
    int n = 5;
    for (int i = 0; i < n; i = i + 1)
    {
        for (int j = 0; j < n - i; j = j + 1)  //打印若干个空格符" "
        {
            printf(" ");
        }
        for (int k = 0; k < i + i - 1; k = k + 1)  //打印若干个"*"
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
#include<stdio.h> 
int main()
{
    int n = 8;
    for (int i = 3; i < n; i = i + 1)
    {
        for (int j = 10; j < i ; j = j + 1)
        {
            printf(" ");
        }
        for (int k = 0; k < i + i - 1; k = k + 1)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}

#include<stdio.h> 
int main()
{
    int n = 8;
    for (int i = 1; i < n; i = i + 1)
    {
        for (int j = 10; j < i; j = j + 1)
        {
            printf(" ");
        }
        for (int k = 0; k < i + i - 1; k = k + 1)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
#include<stdio.h> 
int main()
{
    int n = 4;
    for (int i = 0; i < n; i = i + 1)
    {
        for (int j = 0; j < i; j = j + 1)
        {
            printf(" ");
        }
        for (int k = 0; k < n - i; k = k + 1)
        {
            printf("*");
        }
        printf("\n");
    }
    return  0;
}
#include<stdio.h>
int main()
{
	for (int i = 0; i <= 4; i++)
		printf("*********\n");




	return 0;
}
