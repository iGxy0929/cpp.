#include<stdio.h>
int Max(int a[],int n)
{
	int max=a[0];
	for(int i=0;i<n;i++)
	{
		if(a[i]>max)
		{
			max=a[i];
		}
	}
	return max;
}
int main()
{	
	int n=0;
	int a[n];
	printf("请输入整数的个数：");
	scanf("%d",&n);
	for(int j=0;j<n;j++)
	{
		scanf("%d",&a[j]);
	}
	int max=Max(a,n);
	printf("%d",max);
	return 0;
}
